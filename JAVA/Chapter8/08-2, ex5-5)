package sec02.exam05;

public class Ex {
  public static void main(String[] args) {
		ImplementationC impl = new ImplementationC();
		
		InterfaceA ia = impl;
		ia.methodA();
		System.out.println();      InterfaceA변수는 methodA()만 호출 가능
		
		InterfaceB ib = impl;      InterfaceB변수는 methodB()만 호출 가능
		ib.methodB();
		System.out.println();
		
		InterfaceC ic = impl;
		ic.methodA();
		ic.methodB();
		ic.methodC();
	}
}
