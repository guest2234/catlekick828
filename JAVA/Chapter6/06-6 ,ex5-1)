package sec06.exam05.package1;

public class A {
  //필드
	public int field1;      public접근 제한
	int field2;            default접근 제한
	private int field3;    private접근 제한

  //생성자
	public A() {
		field1 = 1;
		field2 = 1;
		field3 = 1;
		
		method1();
		method2();
		method3();
	}

  //메소드
	public void method1() {}      public접근 제한
	void method2() {}            default접근 제한
	private void method3() {}    prinate접근 제한
}
